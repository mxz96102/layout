{
  "name": "@antv/layout-gpu",
  "version": "1.0.0",
  "description": "layout run on webgpu for antv",
  "keywords": [
    "layout"
  ],
  "author": "多牧 <xuzhi.mxz@antfin.com>",
  "homepage": "https://github.com/antvis/layout/tree/main/packages/layout-gpu#readme",
  "license": "MIT",
  "main": "lib/layout-gpu.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/layout.git"
  },
  "scripts": {
    "clean": "rimraf lib esm dist",
    "lint-staged": "lint-staged",
    "size": "limit-size",
    "lint": "eslint ./src ./__tests__ && prettier ./src ./__tests__ --check ",
    "fix": "eslint ./src ./__tests__ --fix && prettier ./src ./__tests__ --write ",
    "test": "jest",
    "build:umd": "rimraf ./dist && rollup -c",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "ci": "run-s lint test build",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "@antv/g-webgpu": "^0.5.5",
    "@antv/graphlib": "^1.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "babel": "^6.23.0",
    "d3-force": "^2.0.1",
    "ml-matrix": "^6.5.0",
    "rollup-plugin-bundle-size": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-electron": "^0.1.12",
    "lerna": "^4.0.0",
    "limit-size": "^0.1.4",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.6",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "bugs": {
    "url": "https://github.com/antvis/layout/issues"
  }
}
